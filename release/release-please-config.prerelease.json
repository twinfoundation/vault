{
	"$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
	"description": "Auto-Generated Release Please configuration for prerelease",
	"pull-request-title-pattern": "chore: prerelease release prepared",
	"pull-request-header": "chore: prerelease release prepared",
	"pull-request-footer": "This PR was generated by the prepare-release GHA",
	"release-type": "node",
	"initial-version": "0.0.1",
	"versioning": "prerelease",
	"prerelease": true,
	"prerelease-type": "next",
	"packages": {
		"packages/vault-models": {
			"package-name": "vault-models",
			"changelog-path": "docs/changelog.md",
			"extra-files": [
				"src/index.ts"
			]
		},
		"packages/vault-connector-entity-storage": {
			"package-name": "vault-connector-entity-storage",
			"changelog-path": "docs/changelog.md",
			"extra-files": [
				"src/index.ts"
			]
		},
		"packages/vault-connector-hashicorp": {
			"package-name": "vault-connector-hashicorp",
			"changelog-path": "docs/changelog.md",
			"extra-files": [
				"src/index.ts"
			]
		}
	},
	"plugins": [
		"node-workspace",
		{
			"type": "linked-versions",
			"groupName": "repo",
			"components": [
				"vault-models",
				"vault-connector-entity-storage",
				"vault-connector-hashicorp"
			]
		}
	]
}
